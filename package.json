{
  "name": "react-native-hold-menu",
  "version": "0.9.10",
  "description": "A performant, easy to use hold to open context menu for React Native powered by Reanimated.",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "lib"
  ],
  "keywords": [
    "react-native",
    "ios",
    "android",
    "hold-menu",
    "context-menu",
    "hold-to-open",
    "reanimated",
    "menu"
  ],
  "repository": "https://github.com/SohelIslamImran/react-native-hold-menu",
  "author": "Enes Ozturk (https://ozturkenes.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SohelIslamImran/react-native-hold-menu/issues"
  },
  "homepage": "https://github.com/SohelIslamImran/react-native-hold-menu",
  "scripts": {
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "build": "bob build && yarn copy-dts",
    "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" lib/typescript",
    "release": "release-it",
    "version": "auto-changelog -p && git add ."
  },
  "dependencies": {
    "@gorhom/portal": "^1.0.14",
    "expo-blur": "^11.2.0",
    "expo-haptics": "^11.3.0",
    "lodash.isequal": "^4.5.0",
    "nanoid": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "~7.19.0",
    "@react-native-community/eslint-config": "^3.1.0",
    "@release-it/conventional-changelog": "^5.1.0",
    "@types/faker": "^6.6.9",
    "@types/lodash.isequal": "^4.5.6",
    "@types/react": "18.0.0",
    "@types/react-native": "0.69.5",
    "auto-changelog": "^2.4.0",
    "babel-jest": "~29.0.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "react": "18.0.0",
    "react-native": "0.69.5",
    "react-native-builder-bob": "^0.18.3",
    "react-native-gesture-handler": "~2.5.0",
    "react-native-reanimated": "~2.9.1",
    "release-it": "^15.4.1",
    "typescript": "~4.8.2"
  },
  "peerDependencies": {
    "expo-blur": "^11.2.0",
    "expo-haptics": "^11.3.0",
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": ">=2.5.0",
    "react-native-reanimated": ">=2.9.1"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
